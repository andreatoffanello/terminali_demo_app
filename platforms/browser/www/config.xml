<?xml version='1.0' encoding='utf-8'?>
<widget id="com.thetisit.demoautisti" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>eBus Demo</name>
    <description>
        eBus demo driver's app.
    </description>
    <author email="support@thetisit.com" href="https://thetisit.com">
        Thetis IT
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <splash src="www/res/screen/android/drawable-port-xxxhdpi-screen.png" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="14" />
    <preference name="SplashScreen" value="splash" />
    <preference name="StatusBarOverlaysWebView" value="true" />
    <preference name="Fullscreen" value="true" />
    <preference name="Orientation" value="landscape" />
</widget>
